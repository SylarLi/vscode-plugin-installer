{
  "name": "vscode-plugin-installer",
  "displayName": "VS Code Plugin Installer",
  "description": "A VS Code extension to install plugins from Microsoft Marketplace Link or Extension ID",
  "version": "0.0.3",
  "publisher": "Sylar",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SylarLi/vscode-plugins-installer.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "vscode-plugin-installer.installPlugin",
        "title": "Install Plugin from Microsoft Marketplace Link or Extension ID"
      }
    ]
  },
  "activationEvents": [
    "onCommand:vscode-plugin-installer.installPlugin"
  ],
  "dependencies": {
    "jszip": "^3.10.1"
  }
}
